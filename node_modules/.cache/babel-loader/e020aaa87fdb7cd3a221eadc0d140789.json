{"ast":null,"code":"export default {\n  name: \"NotesPage\",\n  data: () => ({\n    id: 0,\n    title: \"\",\n    content: \"\",\n    status: \"\",\n    notesTbl: []\n  }),\n  computed: {\n    sortedList: function () {\n      return this.content.slice().sort(function (a, b) {\n        return b.id - a.id;\n      });\n    }\n  },\n  methods: {\n    onSubmit() {\n      this.id++;\n      this.notesTbl.push({\n        id: this.id,\n        title: this.title,\n        content: this.content,\n        status: \"\"\n      });\n      this.clearForm();\n    },\n\n    clearForm() {\n      this.title = \"\";\n      this.content = \"\";\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgDA,eAAe;EACbA,IAAI,EAAE,WADO;EAEbC,IAAI,EAAE,OAAO;IAAEC,EAAE,EAAE,CAAN;IAASC,KAAK,EAAE,EAAhB;IAAoBC,OAAO,EAAE,EAA7B;IAAiCC,MAAM,EAAE,EAAzC;IAA6CC,QAAQ,EAAE;EAAvD,CAAP,CAFO;EAGbC,QAAQ,EAAE;IACRC,UAAU,EAAE,YAAW;MACrB,OAAO,KAAKJ,OAAL,CAAaK,KAAb,GAAqBC,IAArB,CAA0B,UAASC,CAAT,EAAYC,CAAZ,EAAe;QAC9C,OAAOA,CAAC,CAACV,EAAF,GAAOS,CAAC,CAACT,EAAhB;MACD,CAFM,CAAP;IAGD;EALO,CAHG;EAUbW,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,KAAKZ,EAAL;MACA,KAAKI,QAAL,CAAcS,IAAd,CAAmB;QAAEb,EAAE,EAAE,KAAKA,EAAX;QAAeC,KAAK,EAAE,KAAKA,KAA3B;QAAkCC,OAAO,EAAE,KAAKA,OAAhD;QAAyDC,MAAM,EAAE;MAAjE,CAAnB;MACA,KAAKW,SAAL;IACD,CALM;;IAMPA,SAAS,GAAG;MACV,KAAKb,KAAL,GAAa,EAAb;MACA,KAAKC,OAAL,GAAe,EAAf;IACD;;EATM;AAVI,CAAf","names":["name","data","id","title","content","status","notesTbl","computed","sortedList","slice","sort","a","b","methods","onSubmit","push","clearForm"],"sourceRoot":"","sources":["C:\\Users\\Käyttäjä\\notes\\src\\components\\NotesPage.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h1 class=\"mt-4 text-center\">Add Note</h1>\n    <form>\n      <div class=\"form-group\">\n        <label for=\"title\">Title</label>\n        <input\n          type=\"text\"\n          placeholder=\"Add note\"\n          v-model=\"title\"\n          class=\"form-control\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label for=\"content\">Content</label>\n        <input\n          type=\"text\"\n          placeholder=\"Lorem ipsum\"\n          v-model=\"content\"\n          class=\"form-control\"\n        />\n      </div>\n      <button type=\"button\" @click=\"onSubmit\" class=\"btn btn-dark\">\n        Save\n      </button>\n    </form>\n    <table class=\"table mt-5\">\n      <thead>\n        <tr>\n          <th scope=\"col\">Id</th>\n          <th scope=\"col\">Title</th>\n          <th scope=\"col\">Content</th>\n          <th scope=\"col\">Status</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(entry, i) in sortedList\" :key=\"i\">\n          <td>{{ entry.id }}</td>\n          <td>{{ entry.title }}</td>\n          <td>{{ entry.content }}</td>\n          <td>{{ entry.status }}</td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"NotesPage\",\n  data: () => ({ id: 0, title: \"\", content: \"\", status: \"\", notesTbl: []}),\n  computed: {\n    sortedList: function() {\n      return this.content.slice().sort(function(a, b) {\n        return b.id - a.id;\n      });\n    },\n  },\n  methods: {\n    onSubmit() {\n      this.id++;\n      this.notesTbl.push({ id: this.id, title: this.title, content: this.content, status: \"\" });\n      this.clearForm();\n    },\n    clearForm() {\n      this.title = \"\";\n      this.content = \"\";\n    },\n  },\n};\n</script>"]},"metadata":{},"sourceType":"module"}